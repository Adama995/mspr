pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo "Lancement de l'application"
                sh 'java -jar Users/hp/Documents/ConnectorMspr.jar'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
                sh 'scp Users/hp/Documents/ConnectorMspr.jar/index.html'
            }
        }
    }
}








